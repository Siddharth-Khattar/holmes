{
  "color": {
    "$type": "color",
    "text": {
      "primary": {"$value": "{color.gray.50}"},
      "secondary": {"$value": "{color.gray.500}"},
      "inverse": {"$value": "{color.gray.900}"}
    },
    "background": {
      "primary": {"$value": "{color.gray.900}"},
      "secondary": {"$value": "{color.gray.800}"},
      "tertiary": {"$value": "{color.black}"}
    },
    "border": {
      "default": {"$value": "{color.gray.800}"}
    },
    "surface": {
      "elevated": {
        "$value": {"colorSpace": "srgb", "components": [0.16, 0.16, 0.16], "hex": "#292929"}
      }
    }
  },
  "shadow": {
    "$type": "shadow",
    "sm": {
      "$value": {
        "color": {"colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.3},
        "offsetX": {"value": 0, "unit": "px"},
        "offsetY": {"value": 1, "unit": "px"},
        "blur": {"value": 2, "unit": "px"},
        "spread": {"value": 0, "unit": "px"}
      }
    },
    "md": {
      "$value": {
        "color": {"colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.4},
        "offsetX": {"value": 0, "unit": "px"},
        "offsetY": {"value": 4, "unit": "px"},
        "blur": {"value": 6, "unit": "px"},
        "spread": {"value": -1, "unit": "px"}
      }
    },
    "lg": {
      "$value": [
        {
          "color": {"colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.5},
          "offsetX": {"value": 0, "unit": "px"},
          "offsetY": {"value": 10, "unit": "px"},
          "blur": {"value": 15, "unit": "px"},
          "spread": {"value": -3, "unit": "px"}
        },
        {
          "color": {"colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.3},
          "offsetX": {"value": 0, "unit": "px"},
          "offsetY": {"value": 4, "unit": "px"},
          "blur": {"value": 6, "unit": "px"},
          "spread": {"value": -2, "unit": "px"}
        }
      ]
    }
  }
}
